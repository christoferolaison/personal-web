{
  "name": "webpack-react-static-site-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for creating static web apps with React.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run clean && webpack --config webpack/dev.config -w",
    "build": "npm run clean && webpack --config webpack/deploy.config -p",
    "deploy": "npm run build && gh-pages -d build",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christoferolaison/webpack-react-static-site-boilerplate.git"
  },
  "keywords": [
    "static",
    "site",
    "react",
    "webpack"
  ],
  "author": "Christofer Olaison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christoferolaison/webpack-react-static-site-boilerplate/issues"
  },
  "homepage": "https://github.com/christoferolaison/webpack-react-static-site-boilerplate",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-2": "^7.0.0-beta.40",
    "babel-loader": "^8.0.0-beta.0",
    "browser-sync": "^2.18.6",
    "browser-sync-webpack-plugin": "^1.1.3",
    "gh-pages": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.5.4",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "webpack": "^3.5.5"
  }
}
